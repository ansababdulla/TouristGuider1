@model IEnumerable<TouristGuider.Models.Credential>

@{
    ViewBag.Title = "Index";
}

<p>
    @Html.ActionLink("Create New Admin", "Create")
</p>
<p>
    @Html.ActionLink("Create New Restaurant Owner", "CreateRstOwner")
</p>
<p>
    @Html.ActionLink("Create New Hotel Owner", "CreateHtlOwner")
</p>
<p>
    @Html.ActionLink("Create New Rent Owner", "CreateRenttOwner")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role.RoleNm)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role.RoleNm)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CredID }) |
                @Html.ActionLink("Details", "Details", new { id = item.CredID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.CredID })
            </td>
        </tr>
    }

</table>
